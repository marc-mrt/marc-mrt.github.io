{
  "baseURL": "https://www.mrnt.fr/",
  "author": "Marc Morant",
  "title": "Marc Morant",
  "languageCode": "en-us",
  "enableRobotsTXT": true,
  "permalinks": {
    "/blog/": "/blog/:year:month:day/:title"
  },
  "deployment": {
    "matchers": [
      {
        "pattern": "^.+\\.(js|css|svg|ttf|woff2)$",
        "cacheControl": "max-age=31536000, no-transform, public",
        "gzip": true
      }
    ]
  }
}
